{
  "session_name": "Example Trading Session",
  "exchange_group": "US_EQUITY",
  "asset_class": "EQUITY",
  "mode": "backtest",
  "backtest_config": {
    "start_date": "2025-07-02",
    "end_date": "2025-07-03",
    "speed_multiplier": 60.0
  },
  "session_data_config": {
    "symbols": ["RIVN"],
    "streams": ["1m", "5m", "10m", "quotes"],
    "streaming": {
      "catchup_threshold_seconds": 60,
      "catchup_check_interval": 10
    },
    "historical": {
      "enable_quality": true,
      "data": [
        {
          "trailing_days": 3,
          "intervals": ["1m"],
          "apply_to": "all"
        },
        {
          "trailing_days": 2,
          "intervals": ["1d"],
          "apply_to": "all"
        }
      ],
      "indicators": {
        "avg_volume": {
          "type": "trailing_average",
          "period": "2d",
          "granularity": "daily",
          "field": "volume"
        }
      }
    },
    "gap_filler": {
      "enable_session_quality": true,
      "max_retries": 5,
      "retry_interval_seconds": 60
    }
  },
  "trading_config": {
    "max_buying_power": 100000.0,
    "max_per_trade": 10000.0,
    "max_per_symbol": 20000.0,
    "max_open_positions": 5
  },
  "api_config": {
    "data_api": "alpaca",
    "trade_api": "alpaca"
  },
  "metadata": {
    "created_by": "user",
    "description": "Example session configuration matching SESSION_ARCHITECTURE.md",
    "strategy": "momentum_scalping",
    "version": "2.0"
  }
}
