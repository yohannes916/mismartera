{
  "session_name": "Example Trading Session",
  "mode": "backtest",
  "data_streams": [
    {
      "type": "bars",
      "symbol": "X",
      "interval": "1m"
    },
    {
      "type": "bars",
      "symbol": "Y",
      "interval": "1m"
    },
    {
      "type": "bars",
      "symbol": "Z",
      "interval": "5m"
    },
    {
      "type": "quotes",
      "symbol": "Z"
    }
